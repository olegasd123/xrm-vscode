{
  "name": "dynamics-365-tools",
  "displayName": "Dynamics 365 Tools",
  "description": "Publish Dynamics 365 web resources directly from VS Code with environment-aware bindings.",
  "version": "1.0.10",
  "publisher": "oleg-verhoglyad",
  "icon": "assets/icon.png",
  "license": "MIT",
  "keywords": [
    "dynamics 365",
    "crm",
    "power apps",
    "web resources",
    "publishing"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/olegasd123/dynamics-365-tools"
  },
  "bugs": {
    "url": "https://github.com/olegasd123/dynamics-365-tools/issues"
  },
  "homepage": "https://github.com/olegasd123/dynamics-365-tools#readme",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:dynamics365Tools.openResourceMenu",
    "onCommand:dynamics365Tools.configureEnvironments",
    "onCommand:dynamics365Tools.bindResource",
    "onCommand:dynamics365Tools.openInCrm",
    "onCommand:dynamics365Tools.publishResource",
    "onCommand:dynamics365Tools.signInInteractive",
    "onCommand:dynamics365Tools.signOut",
    "onCommand:dynamics365Tools.setEnvironmentCredentials",
    "onCommand:dynamics365Tools.plugins.registerAssembly",
    "onCommand:dynamics365Tools.plugins.updateAssembly",
    "onCommand:dynamics365Tools.plugins.generatePublicKeyToken",
    "onCommand:dynamics365Tools.plugins.createStep",
    "onCommand:dynamics365Tools.plugins.editStep",
    "onCommand:dynamics365Tools.plugins.deleteStep",
    "onCommand:dynamics365Tools.plugins.copyStepDescription",
    "onCommand:dynamics365Tools.plugins.createImage",
    "onCommand:dynamics365Tools.plugins.editImage",
    "onCommand:dynamics365Tools.plugins.deleteImage",
    "onCommand:dynamics365Tools.plugins.copyImageDescription",
    "onCommand:dynamics365Tools.plugins.deletePluginType",
    "onCommand:dynamics365Tools.plugins.toggleSolutionFilter",
    "onCommand:dynamics365Tools.plugins.enableSolutionFilter",
    "onCommand:dynamics365Tools.plugins.disableSolutionFilter",
    "onCommand:dynamics365Tools.plugins.refreshExplorer",
    "onCommand:dynamics365Tools.plugins.publishLastAssembly",
    "onCommand:dynamics365Tools.publishLastResource",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dynamics365Tools.openResourceMenu",
        "title": "Resource Actions",
        "category": "Dynamics 365 Tools"
      },
      {
        "command": "dynamics365Tools.openInCrm",
        "title": "Open in Power Apps",
        "category": "Dynamics 365 Tools"
      },
      {
        "command": "dynamics365Tools.configureEnvironments",
        "title": "Edit Environments & Solutions",
        "category": "Dynamics 365 Tools",
        "icon": "$(gear)"
      },
      {
        "command": "dynamics365Tools.bindResource",
        "title": "Bind Resource",
        "category": "Dynamics 365 Tools"
      },
      {
        "command": "dynamics365Tools.publishResource",
        "title": "Publish Resource",
        "category": "Dynamics 365 Tools"
      },
      {
        "command": "dynamics365Tools.publishLastResource",
        "title": "Publish Last Resource",
        "category": "Dynamics 365 Tools"
      },
      {
        "command": "dynamics365Tools.setEnvironmentCredentials",
        "title": "Set Environment Credentials",
        "category": "Dynamics 365 Tools"
      },
      {
        "command": "dynamics365Tools.signInInteractive",
        "title": "Sign In (Interactive)",
        "category": "Dynamics 365 Tools"
      },
      {
        "command": "dynamics365Tools.signOut",
        "title": "Sign Out",
        "category": "Dynamics 365 Tools"
      },
      {
        "command": "dynamics365Tools.plugins.registerAssembly",
        "title": "Register Plugin Assembly",
        "category": "Dynamics 365 Tools",
        "icon": "$(cloud-upload)"
      },
      {
        "command": "dynamics365Tools.plugins.updateAssembly",
        "title": "Update Plugin Assembly",
        "category": "Dynamics 365 Tools",
        "icon": "$(cloud-download)"
      },
      {
        "command": "dynamics365Tools.plugins.publishLastAssembly",
        "title": "Publish Last Plugin Assembly",
        "category": "Dynamics 365 Tools",
        "icon": "$(package)"
      },
      {
        "command": "dynamics365Tools.plugins.deletePluginType",
        "title": "Delete Plugin",
        "category": "Dynamics 365 Tools",
        "icon": "$(trash)"
      },
      {
        "command": "dynamics365Tools.plugins.refreshExplorer",
        "title": "Refresh Plugin Explorer",
        "category": "Dynamics 365 Tools",
        "icon": "$(refresh)"
      },
      {
        "command": "dynamics365Tools.plugins.toggleSolutionFilter",
        "title": "Show Configured Solutions Only",
        "category": "Dynamics 365 Tools",
        "icon": "$(filter)"
      },
      {
        "command": "dynamics365Tools.plugins.enableSolutionFilter",
        "title": "Show Configured Solutions Only",
        "category": "Dynamics 365 Tools",
        "icon": "$(filter)"
      },
      {
        "command": "dynamics365Tools.plugins.disableSolutionFilter",
        "title": "Show Configured Solutions Only",
        "category": "Dynamics 365 Tools",
        "icon": "$(filter-filled)"
      },
      {
        "command": "dynamics365Tools.plugins.generatePublicKeyToken",
        "title": "Generate Strong Name Key (Public Key Token)",
        "category": "Dynamics 365 Tools",
        "icon": "$(key)"
      },
      {
        "command": "dynamics365Tools.plugins.createStep",
        "title": "Create Plugin Step",
        "category": "Dynamics 365 Tools",
        "icon": "$(add)"
      },
      {
        "command": "dynamics365Tools.plugins.copyStepDescription",
        "title": "Copy Step Info",
        "category": "Dynamics 365 Tools",
        "icon": "$(copy)"
      },
      {
        "command": "dynamics365Tools.plugins.editStep",
        "title": "Edit Plugin Step",
        "category": "Dynamics 365 Tools",
        "icon": "$(edit)"
      },
      {
        "command": "dynamics365Tools.plugins.enableStep",
        "title": "Enable Plugin Step",
        "category": "Dynamics 365 Tools",
        "icon": "$(debug-start)"
      },
      {
        "command": "dynamics365Tools.plugins.disableStep",
        "title": "Disable Plugin Step",
        "category": "Dynamics 365 Tools",
        "icon": "$(debug-pause)"
      },
      {
        "command": "dynamics365Tools.plugins.deleteStep",
        "title": "Delete Plugin Step",
        "category": "Dynamics 365 Tools",
        "icon": "$(trash)"
      },
      {
        "command": "dynamics365Tools.plugins.createImage",
        "title": "Create Plugin Image",
        "category": "Dynamics 365 Tools",
        "icon": "$(file-media)"
      },
      {
        "command": "dynamics365Tools.plugins.copyImageDescription",
        "title": "Copy Plugin Image Info",
        "category": "Dynamics 365 Tools",
        "icon": "$(copy)"
      },
      {
        "command": "dynamics365Tools.plugins.editImage",
        "title": "Edit Plugin Image",
        "category": "Dynamics 365 Tools",
        "icon": "$(edit)"
      },
      {
        "command": "dynamics365Tools.plugins.deleteImage",
        "title": "Delete Plugin Image",
        "category": "Dynamics 365 Tools",
        "icon": "$(trash)"
      }
    ],
    "submenus": [
      {
        "id": "dynamics365Tools.resourceMenu",
        "label": "Dynamics 365 Tools"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "submenu": "dynamics365Tools.resourceMenu",
          "group": "navigation@dynamics365Tools",
          "when": "resourceScheme == 'file' && (explorerResourceIsFolder || resourceExtname =~ /\\.(js|css|html|htm|xml|json|resx|png|jpg|jpeg|gif|xsl|xslt|ico|svg)$/)"
        }
      ],
      "dynamics365Tools.resourceMenu": [
        {
          "command": "dynamics365Tools.publishResource",
          "group": "inline@1"
        },
        {
          "command": "dynamics365Tools.bindResource",
          "group": "inline@2"
        },
        {
          "command": "dynamics365Tools.openInCrm",
          "group": "inline@3",
          "when": "true"
        },
        {
          "command": "dynamics365Tools.configureEnvironments",
          "group": "inline@4"
        }
      ],
      "commandPalette": [
        {
          "command": "dynamics365Tools.openResourceMenu"
        },
        {
          "command": "dynamics365Tools.openInCrm"
        },
        {
          "command": "dynamics365Tools.configureEnvironments"
        },
        {
          "command": "dynamics365Tools.bindResource"
        },
        {
          "command": "dynamics365Tools.publishResource"
        },
        {
          "command": "dynamics365Tools.publishLastResource"
        },
        {
          "command": "dynamics365Tools.setEnvironmentCredentials"
        },
        {
          "command": "dynamics365Tools.signInInteractive"
        },
        {
          "command": "dynamics365Tools.signOut"
        },
        {
          "command": "dynamics365Tools.plugins.registerAssembly"
        },
        {
          "command": "dynamics365Tools.plugins.updateAssembly"
        },
        {
          "command": "dynamics365Tools.plugins.publishLastAssembly"
        },
        {
          "command": "dynamics365Tools.plugins.deletePluginType"
        },
        {
          "command": "dynamics365Tools.plugins.refreshExplorer"
        },
        {
          "command": "dynamics365Tools.plugins.toggleSolutionFilter"
        },
        {
          "command": "dynamics365Tools.plugins.generatePublicKeyToken"
        },
        {
          "command": "dynamics365Tools.plugins.createStep"
        },
        {
          "command": "dynamics365Tools.plugins.editStep"
        },
        {
          "command": "dynamics365Tools.plugins.enableStep"
        },
        {
          "command": "dynamics365Tools.plugins.disableStep"
        },
        {
          "command": "dynamics365Tools.plugins.deleteStep"
        },
        {
          "command": "dynamics365Tools.plugins.createImage"
        },
        {
          "command": "dynamics365Tools.plugins.editImage"
        },
        {
          "command": "dynamics365Tools.plugins.deleteImage"
        }
      ],
      "view/title": [
        {
          "command": "dynamics365Tools.configureEnvironments",
          "when": "view == dynamics365Tools.pluginExplorer",
          "group": "navigation@1"
        },
        {
          "command": "dynamics365Tools.plugins.disableSolutionFilter",
          "when": "view == dynamics365Tools.pluginExplorer && d365Tools.plugins.filterConfiguredSolutions",
          "group": "navigation@2",
          "toggled": "d365Tools.plugins.filterConfiguredSolutions"
        },
        {
          "command": "dynamics365Tools.plugins.enableSolutionFilter",
          "when": "view == dynamics365Tools.pluginExplorer && !d365Tools.plugins.filterConfiguredSolutions",
          "group": "navigation@2",
          "toggled": "d365Tools.plugins.filterConfiguredSolutions"
        },
        {
          "command": "dynamics365Tools.plugins.refreshExplorer",
          "when": "view == dynamics365Tools.pluginExplorer",
          "group": "navigation@3"
        },
        {
          "command": "dynamics365Tools.plugins.registerAssembly",
          "when": "view == dynamics365Tools.pluginExplorer",
          "group": "navigation@4"
        }
      ],
      "view/item/context": [
        {
          "command": "dynamics365Tools.plugins.updateAssembly",
          "when": "view == dynamics365Tools.pluginExplorer && viewItem == d365PluginAssembly",
          "group": "inline"
        },
        {
          "command": "dynamics365Tools.plugins.createStep",
          "when": "view == dynamics365Tools.pluginExplorer && viewItem == d365PluginType",
          "group": "inline@1"
        },
        {
          "command": "dynamics365Tools.plugins.deletePluginType",
          "when": "view == dynamics365Tools.pluginExplorer && viewItem == d365PluginType",
          "group": "inline@2"
        },
        {
          "command": "dynamics365Tools.plugins.copyStepDescription",
          "when": "view == dynamics365Tools.pluginExplorer && viewItem =~ /d365PluginStep/",
          "group": "inline@0"
        },
        {
          "command": "dynamics365Tools.plugins.editStep",
          "when": "view == dynamics365Tools.pluginExplorer && viewItem =~ /d365PluginStep/",
          "group": "inline@1"
        },
        {
          "command": "dynamics365Tools.plugins.createImage",
          "when": "view == dynamics365Tools.pluginExplorer && viewItem =~ /d365PluginStep/",
          "group": "inline@2"
        },
        {
          "command": "dynamics365Tools.plugins.enableStep",
          "when": "view == dynamics365Tools.pluginExplorer && viewItem == d365PluginStep:disabled",
          "group": "inline@3"
        },
        {
          "command": "dynamics365Tools.plugins.disableStep",
          "when": "view == dynamics365Tools.pluginExplorer && viewItem == d365PluginStep:enabled",
          "group": "inline@3"
        },
        {
          "command": "dynamics365Tools.plugins.deleteStep",
          "when": "view == dynamics365Tools.pluginExplorer && viewItem =~ /d365PluginStep/",
          "group": "inline@4"
        },
        {
          "command": "dynamics365Tools.plugins.copyImageDescription",
          "when": "view == dynamics365Tools.pluginExplorer && viewItem == d365PluginImage",
          "group": "inline@0"
        },
        {
          "command": "dynamics365Tools.plugins.editImage",
          "when": "view == dynamics365Tools.pluginExplorer && viewItem == d365PluginImage",
          "group": "inline@1"
        },
        {
          "command": "dynamics365Tools.plugins.deleteImage",
          "when": "view == dynamics365Tools.pluginExplorer && viewItem == d365PluginImage",
          "group": "inline@2"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "dynamics365tools-utility",
          "title": "Dynamics 365 Tools",
          "icon": "assets/icon.png"
        }
      ]
    },
    "views": {
      "dynamics365tools-utility": [
        {
          "id": "dynamics365Tools.pluginExplorer",
          "name": "Plugins",
          "contextualTitle": "Dynamics 365 Tools"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "npm run compile && node ./tests/run-tests.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "@types/node": "^20.10.2",
    "@types/vscode": "^1.84.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "zod": "^4.1.13"
  }
}
